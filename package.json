    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.24.0",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.45.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.4",
    "ts-jest-resolver": "^1.0.0",
    "tslib": "^2.2.0",
    "typedoc": "^0.20.35",
